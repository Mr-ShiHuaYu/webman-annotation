{
  "name": "ysh/webman-annotation",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/x2nx/webman-annotation",
  "description": "Webman Annotation plugin - Production-ready annotation system for webman framework with security, performance and reliability",
  "keywords": [
    "webman",
    "annotation",
    "attribute",
    "plugin",
    "routing",
    "middleware",
    "dependency-injection",
    "php8"
  ],
  "authors": [
    {
      "name": "OK Xaas",
      "email": "okxaas@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "workerman/webman-framework": "^2.0",
    "webman/cache": "^2.0"
  },
  "suggest": {
    "webman/log": "^2.0",
    "webman/event": "^1.0",
    "webman/channel": "^1.0",
    "workerman/crontab": "^1.0",
    "psr/container": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "X2nx\\WebmanAnnotation\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "X2nx\\WebmanAnnotation\\Tests\\": "tests"
    }
  },
  "extra": {
    "webman": {
      "config": "config/plugin/x2nx/webman-annotation"
    }
  },
  "support": {
    "issues": "https://github.com/x2nx/webman-annotation/issues",
    "source": "https://github.com/x2nx/webman-annotation"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
